<tr>
  <td><%= user.username %></td>
  <td><%= user.fullname %></td>
  <td><%= user.email %></td>
  <td>
    <%= link_to "Edit", edit_user_path(user), class: "btn btn-primary" %>
    <%= link_to "Delete", user_path(user), method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-primary" %>
</tr>
