<!DOCTYPE html>
<html>
<head>
  <title>SIPPMA | Simple Private Practice Managment</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body class="<%= @login_page ? 'login-page' : '' %>">
  <%= render 'layouts/header' unless @print %>
  <%= yield %>

  <% unless @login_page %>
    <%= render 'layouts/footer' unless @print %>
    <%= debug(params) if Rails.env.development? and not @print %>
  <% end %>
</body>
</html>
